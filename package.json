    "clean": "gatsby clean"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "cross-env": "^7.0.3",
    "gatsby": "^3.3.1",
    "gatsby-plugin-local-search": "^2.0.1",
    "gatsby-plugin-netlify-cms": "^5.3.0",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-remove-fingerprints": "0.0.2",
    "gatsby-plugin-sass": "^4.3.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-remark": "^4.1.0",
    "netlify-cms-app": "^2.14.46",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-checkbox-tree": "^1.6.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-use-flexsearch": "^0.1.1",
    "sass": "^1.32.11"
